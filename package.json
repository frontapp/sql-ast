{
  "name": "sql-ast",
  "version": "0.3.0",
  "license": "MIT",
  "repository": "aleclarson/sql-ast",
  "description": "MySQL parser",
  "scripts": {
    "convert": "npx run index.js"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-plugin-node": "^7.0.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-cond-assign": 0,
      "no-constant-condition": 0,
      "object-curly-spacing": [
        2,
        "never"
      ]
    }
  }
}
